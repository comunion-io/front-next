{
  "name": "@comunion/utils",
  "version": "0.0.0",
  "description": "Utility functions",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "./types",
  "scripts": {
    "start": "",
    "esm": "tsc -m esnext && find ./dist -name \"*.js\" | sed 'p;s/\\.js$/\\.esm.js/' | xargs -n2 mv",
    "cjs": "tsc -t es5 -m commonjs",
    "build": "rm -rf dist/* types/* && npm run esm && npm run cjs"
  },
  "repository": "comunion-io/front-next.git",
  "keywords": [
    "typescript",
    "utils"
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/comunion-io/front-next/issues"
  },
  "homepage": "https://github.com/comunion-io/front-next#readme",
  "devDependencies": {
    "@types/node": "^14.14.31",
    "windicss": "^3.1.3"
  }
}
